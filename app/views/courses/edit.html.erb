<% if !flash[:notice].blank? %>
  <div class="notice">
    <%= flash[:notice] %>
  </div>
  <% end %>


<div class="center jumbotron">
 <h1>Update Course</h1>
 
 <%= form_for @course do |f| %>
  <table>
    <tr><td>Player</td><%= render 'scorecards/hole_number_headings' %></tr>
        <%= render 'pars_input' %>
        
  </table>
       <td class="form-buttons"><%= f.submit("Save Edit") %></td>  
<% end %>
      
 <%= link_to "back to courses listing", courses_path %>

</div>